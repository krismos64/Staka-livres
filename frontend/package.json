    "@tanstack/react-query": "^5.81.5",
    "@testing-library/dom": "^10.4.0",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
